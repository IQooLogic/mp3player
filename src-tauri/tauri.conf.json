{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WinampClone",
  "version": "0.1.0",
  "identifier": "com.winamp.clone",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Music Player",
        "width": 500,
        "height": 855,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "minWidth": 500,
        "minHeight": 855
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}